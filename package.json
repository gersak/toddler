{
  "name": "toddler",
  "version": "0.1",
  "description": "UI elements based on helix library",
  "main": "index.js",
  "scripts": {
    "dev": "npx shadow-cljs -A:shadow:cider:showcase watch dev",
    "icons": "npx shadow-cljs -A:shadow:cider:icons watch icons",
    "release": "npx shadow-cljs -A:shadow release webpack",
    "webpack": "npm run release && npx webpack --config webpack.config.js",
    "check": "npx shadow-cljs -A:shadow check dev",
    "init": "npx shadow-cljs -A:shadow:cider compile storybook"
  },
  "author": "Robert Gersak",
  "license": "ISC",
  "devDependencies": {
    "prosemirror-keymap": "^1.2.2",
    "prosemirror-markdown": "^1.13.1",
    "prosemirror-state": "^1.4.3",
    "prosemirror-transform": "^1.10.2",
    "react-icons": "^4.7.1",
    "server": "^1.0.37",
    "shadow-cljs": "^2.20.2"
  },
  "dependencies": {
    "prosemirror-example-setup": "^1.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simplebar-react": "^3.3.0"
  }
}
