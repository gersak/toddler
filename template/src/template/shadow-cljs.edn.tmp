{:deps true
 :dev-http {8000 ["dev"]}
 :builds {:dev {:target :browser
                :entries [{{project}}.main]
                :module-loader true
                :modules {:main {:entries [{{project}}.main]
                                 :init-fn {{project}}.main/start!}
                          :markdown {:entries [toddler.md]
                                     :depends-on #{:main}}
                          :docs {:entries [{{project}}.docs]
                                 :depends-on #{:main}}}
                :output-dir "dev/js"}
          :release {:target :browser
                    :module-loader true
                    :output-dir "target/js"}}}
