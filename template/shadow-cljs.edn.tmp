{:deps true
 :dev-http {8000 ["dev"]}
 :builds {:dev
          {:target :browser
           :entries [{{project}}.main]
           :module-loader true
           :modules {:main {:entries [{{project}}.main]
                            :init-fn {{project}}.main/start!}
                     :markdown {:entries [toddler.md]
                                :depends-on #{:main}}}
           :output-dir "dev/js"
           :output-to "dev/js/main.js"}
          :production
          {:target :browser
           :module-loader true
           :entries [{{project}}.main]
           :modules {:main {:entries [{{project}}.main]
                            :init-fn {{project}}.main/start!}
                     :markdown {:entries [toddler.md]
                                :depends-on #{:main}}}
           :output-dir "build/js"
           :output-to "build/js/main.js"}}}
